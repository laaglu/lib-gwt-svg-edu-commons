<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field='constants' type='org.vectomatic.svg.edu.client.commons.CommonConstants' />
	<g:VerticalPanel>
		<g:Label text="{constants.confirmRestart}" />
		<g:HorizontalPanel>
			<g:Button text="{constants.confirmYes}" ui:field='confirmYesButton' />
			<g:Button text="{constants.confirmNo}" ui:field='confirmNoButton' />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
	